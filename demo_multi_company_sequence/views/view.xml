<?xml version="1.0"?>
<odoo>

  <record id="view_form_demo_sequence" model="ir.ui.view">
    <field name="name">Demo Sequence Form</field>
    <field name="model">demo.sequence</field>
    <field name="arch" type="xml">
      <form>
        <group>
          <field name="name"/>
        </group>
      </form>
    </field>
  </record>

  <record id="view_tree_demo_sequence" model="ir.ui.view">
    <field name="name">Demo Sequence List</field>
    <field name="model">demo.sequence</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name"/>
        </tree>
    </field>
  </record>

  <record id="action_demo_sequence" model="ir.actions.act_window">
      <field name="name">Demo Sequence</field>
      <field name="res_model">demo.sequence</field>
      <field name="view_mode">tree,form</field>
  </record>

</odoo>